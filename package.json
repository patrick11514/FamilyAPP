        "dotenv": "^16.4.7",
        "jsonwebtoken": "^9.0.2",
        "kysely": "^0.27.5",
        "mysql2": "^3.12.0",
        "showdown": "^2.1.0",
        "simple-json-db": "^2.0.0",
        "sweetalert2": "^11.15.10",
