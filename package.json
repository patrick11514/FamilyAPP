        "@types/bcrypt": "^5.0.2",
        "@types/jsonwebtoken": "^9.0.8",
        "@types/showdown": "^2.0.6",
        "@types/uuid": "^10.0.0",
        "@types/web-push": "^3.6.4",
        "eslint": "^9.19.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-svelte": "^2.46.1",
        "globals": "^15.14.0",
        "kysely-codegen": "^0.17.0",
